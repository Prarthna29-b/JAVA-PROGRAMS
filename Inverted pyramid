/*package whatever //do not write package name here */

import java.io.*;

class GFG {
	public static void main (String[] args) {
	    
	   for(int i=5; i<=5&&i>0; i--) {
	       int k=0;
	       for(int j=1;j<=5-i;j++)
	           System.out.print(" ");
	           while(k<2*i-1){
	            System.out.print("*"); 
	           k++;
	       }
		System.out.println();
	
	   }
	   }
}
