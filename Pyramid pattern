import java.io.*;

class GFG {
	public static void main (String[] args) {
	   for(int i=1; i<=5; i++) {
	       int k=1;
	       for(int j=1;j<=5-i;j++)
	           System.out.print(" ");
	           while(k<=2*i-1){
	            System.out.print("*"); 
	           k++;
	       }
	   
		System.out.println();
	
	   }
	   }
}
